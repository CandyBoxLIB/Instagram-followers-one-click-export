{
  "manifest_version": 3,
  "name": "Instagram followers one click export",
  "version": "1.0.1",
  "action": {
    "default_title": "Instagram followers one click export",
    "default_popup": "popup.html",
    "default_icon" : "static/icon.png"
  },
  "icons": {
    "16": "static/logo192.png",
    "48": "static/logo192.png",
    "128": "static/logo192.png"
  },
  "background": {
    "service_worker": "background.out.js"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs",
    "activeTab",
    "scripting",
    "webRequest",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.out.js"],
      "run_at": "document_end"
    }
  ]
}
